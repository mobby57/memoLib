{
  "version": 2,
  "name": "iapostemanager",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/v2/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1",
    "FLASK_ENV": "production"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.11",
      "maxDuration": 10
    }
  },
  "regions": ["cdg1"]
}