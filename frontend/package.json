{
  "name": "iapostemanager-frontend",
  "version": "2.3.0",
  "description": "Frontend React pour IA Poste Manager - MS CONSEILS",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
