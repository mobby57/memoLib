{
  "name": "MemoLib Guardian",
  "url": "https://app.memolib.fr",
  "hook_attributes": {
    "url": "https://app.memolib.fr/api/github/webhook",
    "active": true
  },
  "redirect_url": "https://app.memolib.fr/api/github/callback",
  "public": false,
  "default_permissions": {
    "actions": "read",
    "contents": "read",
    "issues": "read",
    "pull_requests": "read",
    "metadata": "read",
    "email_addresses": "read",
    "profile": "read"
  },
  "default_events": [
    "push",
    "pull_request",
    "issues",
    "issue_comment",
    "workflow_run",
    "check_run",
    "repository",
    "member"
  ]
}
