    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.6.2",
    "browserslist": "^4.28.1",
