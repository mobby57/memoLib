{
  "name": "memolib",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:legacy": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev --turbo",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:turbo": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build --turbo",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NEXT_TELEMETRY_DISABLED=1 next build --turbo",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start -p 3000",
    "preview": "npm run build && npm run start",
    "lint": "eslint src --max-warnings 50",
    "lint:fix": "eslint src --fix",
    "lint:strict": "eslint src --max-warnings 0",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx @cloudflare/next-on-pages --watch",
    "pages:preview": "wrangler pages dev .vercel/output/static",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "cf:deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-cloudflare-direct.ps1",
    "cf:dev": "npm run build && wrangler pages deploy .next/standalone --branch=develop",
    "cf:staging": "npm run build && wrangler pages deploy .next/standalone --branch=staging",
    "cf:prod": "npm run build && wrangler pages deploy .next/standalone",
    "cf:list": "wrangler pages project list",
    "cf:deployments": "wrangler pages deployment list",
    "cf:logs": "wrangler pages deployment tail",
    "cf:status": "wrangler whoami && wrangler pages project list",
    "test": "cross-env NODE_OPTIONS='--max-old-space-size=4096' jest --passWithNoTests --maxWorkers=50%",
    "test:watch": "cross-env NODE_OPTIONS='--max-old-space-size=4096' jest --watch --verbose --maxWorkers=2",
    "test:coverage": "cross-env NODE_OPTIONS='--max-old-space-size=4096' jest --coverage --passWithNoTests --maxWorkers=50%",
    "test:ci": "cross-env NODE_OPTIONS='--max-old-space-size=4096' JEST_SKIP_BILLING=1 JEST_SKIP_SECURITY_2FA=1 jest --coverage --ci --watchAll=false --passWithNoTests --maxWorkers=2 --coverageReporters=json-summary --coverageReporters=lcov --coverageReporters=text",
    "test:integration": "cross-env NODE_OPTIONS='--max-old-space-size=4096' jest __testPathPatterns=integration --coverage --passWithNoTests --maxWorkers=2",
    "test:unit": "cross-env NODE_OPTIONS='--max-old-space-size=4096' jest __testPathPatterns=__tests__ --passWithNoTests --maxWorkers=50%",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"lines\":30,\"branches\":30,\"functions\":30,\"statements\":30}}'",
    "coverage:report": "jest --coverage --coverageReporters=html && start coverage/lcov-report/index.html",
    "test:smoke": "node scripts/smoke-tests.js",
    "test:health": "node scripts/health-check.js",
    "analyze": "cross-env ANALYZE=true NEXT_TELEMETRY_DISABLED=1 next build",
    "analyze:bundle": "npm run analyze",
    "analyze:maps": "ANALYZE=true npm run build && npx source-map-explorer '.next/static/chunks/*.js' --html --output bundle-report.html",
    "type-check": "cross-env NODE_OPTIONS=--max-old-space-size=16384 tsc --noEmit --incremental --skipLibCheck",
    "type-check:safe": "bash scripts/type-check-safe.sh",
    "type-check:changed": "bash scripts/type-check-changed.sh",
    "type-check:diagnostic": "bash scripts/typescript-diagnostic.sh",
    "type-check:watch": "cross-env NODE_OPTIONS=--max-old-space-size=16384 tsc --noEmit --watch",
    "clean": "rimraf .next out dist .turbo node_modules/.cache",
    "clean:all": "rimraf .next out dist .turbo node_modules/.cache coverage",
    "fresh": "npm run clean && npm install && npm run build",
    "figma": "node scripts/figma-cli.js",
    "figma:sync": "tsx scripts/figma-sync.ts",
    "figma:icons": "tsx scripts/figma-icons.ts",
    "figma:all": "npm run figma:sync && npm run figma:icons",
    "prepare": "echo Skipping husky install",
    "postinstall": "prisma generate",
    "db:reset": "node scripts/reset-db.js",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:complete": "tsx prisma/seed-complete.ts",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:optimize": "tsx scripts/db-optimize.ts",
    "db:health": "tsx scripts/db-health.ts",
    "db:benchmark": "tsx scripts/db-benchmark.ts",
    "db:backup": "tsx scripts/db-backup.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "validate": "npm run type-check && npm run lint && npm run test",
    "validate:full": "npm run type-check && npm run lint:strict && npm run test:ci",
    "validate:project": "bash scripts/validate-project.sh",
    "build:azure": "cross-env AZURE_STATIC_WEB_APPS=true TURBOPACK=0 NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:azure:static": "cross-env AZURE_STATIC_EXPORT=true TURBOPACK=0 NODE_OPTIONS=--max-old-space-size=8192 next build",
    "deploy:azure": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-azure-swa.ps1",
    "deploy:azure:static": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-azure-swa.ps1 -Static",
    "deploy:vercel": "vercel --prod",
    "deploy:vercel:preview": "vercel",
    "email:monitor": "tsx scripts/start-gmail-monitor.ts",
    "email:monitor:integrated": "tsx scripts/email-monitor-integrated.ts",
    "email:stats": "tsx scripts/email-stats.ts",
    "email:export": "tsx scripts/email-export.ts",
    "email:setup": "tsx scripts/email-setup.ts",
    "email:to-workspace": "tsx scripts/email-to-workspace.ts",
    "email:to-workspace:ai": "tsx scripts/email-to-workspace-ai.ts",
    "gmail:auth": "node scripts/gmail-auth.js",
    "ai:test": "tsx scripts/ai-test.ts",
    "ai:test:ceseda": "tsx scripts/ai-test-ceseda.ts",
    "seed:demo": "tsx prisma/seeds/demo-workspaces.ts",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force",
    "security:scan": "ggshield secret scan path .",
    "security:precommit": "ggshield secret scan pre-commit",
    "security:ci": "ggshield secret scan ci",
    "security:staged": "ggshield secret scan pre-receive",
    "security:install": "ggshield install -m local",
    "security:check": "ggshield secret scan path . --exit-zero",
    "security:audit": "npm audit && node scripts/security-monitor.js",
    "security:fix": "npm audit fix --force",
    "security:report": "node scripts/security-monitor.js",
    "pre-push": "npm run validate",
    "check-quality": "node scripts/check-quality.js",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "dx:setup": "npm install && npx prisma generate && npm run dev",
    "dx:reset": "npm run clean && npm run dx:setup",
    "dx:check": "npm run lint && npm run type-check && npm run test",
    "dx:health": "node -e \"console.log(\"âœ… Node:\", process.version)\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
