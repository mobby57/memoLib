    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "cloudflare": "^5.2.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "exceljs": "^4.4.0",
    "googleapis": "^169.0.0",
    "ioredis": "^5.9.1",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "mailparser": "^3.9.1",
    "mammoth": "^1.11.0",
    "next": "^16.1.4",
    "next-auth": "^4.24.13",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "openai": "^6.15.0",
    "otplib": "^13.0.0",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^4.0.0",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.69.0",
    "react-icons": "^5.5.0",
    "react-is": "^19.2.3",
    "recharts": "^3.6.0",
    "regression": "^2.0.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "stripe": "^20.2.0",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.58"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260113.0",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@figma/code-connect": "^1.3.12",
    "@next/bundle-analyzer": "15.5.9",
    "@playwright/test": "^1.57.0",
