{
  "name": "iapostemanage-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:turbo": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build --turbo",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx @cloudflare/next-on-pages --watch",
    "pages:preview": "wrangler pages dev .vercel/output/static",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "cloudflare:deploy": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-cloudflare-direct.ps1",
    "cloudflare:dev": "npm run build && npx wrangler pages deploy .next/standalone --branch=develop",
    "cloudflare:staging": "npm run build && npx wrangler pages deploy .next/standalone --branch=staging",
    "cloudflare:prod": "npm run build && npx wrangler pages deploy .next/standalone",
    "cloudflare:list": "npx wrangler pages project list",
    "cloudflare:deployments": "npx wrangler pages deployment list",
    "cloudflare:logs": "npx wrangler pages deployment tail",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --coverage --ci --watchAll=false --passWithNoTests --maxWorkers=2 --coverageReporters=json-summary --coverageReporters=lcov --coverageReporters=text",
    "test:integration": "jest --testPathPattern=integration --coverage --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__ --passWithNoTests",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration",
    "coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"lines\":30,\"branches\":30,\"functions\":30,\"statements\":30}}'",
    "coverage:report": "jest --coverage --coverageReporters=html && start coverage/lcov-report/index.html",
    "test:smoke": "node scripts/smoke-tests.js",
    "test:health": "node scripts/health-check.js",
    "analyze": "cross-env ANALYZE=true next build",
    "build:analyze": "npm run analyze",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out dist",
    "figma": "node scripts/figma-cli.js",
    "figma:sync": "tsx scripts/figma-sync.ts",
    "figma:icons": "tsx scripts/figma-icons.ts",
    "figma:all": "npm run figma:sync && npm run figma:icons",
    "prepare": "echo Skipping husky install",
    "postinstall": "npx prisma generate",
    "db:reset": "node scripts/reset-db.js",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed:complete": "npx tsx prisma/seed-complete.ts",
    "db:studio": "npx prisma studio",
    "db:optimize": "tsx scripts/db-optimize.ts",
    "db:health": "tsx scripts/db-health.ts",
    "db:benchmark": "tsx scripts/db-benchmark.ts",
    "db:backup": "tsx scripts/db-backup.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "validate": "npm run type-check && npm run lint && npm run test:ci",
    "build:azure": "cross-env AZURE_STATIC_WEB_APPS=true TURBOPACK=0 NODE_OPTIONS=--max-old-space-size=4096 next build",
    "build:azure:static": "cross-env AZURE_STATIC_EXPORT=true TURBOPACK=0 NODE_OPTIONS=--max-old-space-size=4096 next build",
    "deploy:azure": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-azure-swa.ps1",
    "deploy:azure:static": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-azure-swa.ps1 -Static",
    "email:monitor": "tsx scripts/email-monitor.ts",
    "email:monitor:integrated": "tsx scripts/email-monitor-integrated.ts",
    "email:stats": "tsx scripts/email-stats.ts",
    "email:export": "tsx scripts/email-export.ts",
    "email:setup": "tsx scripts/email-setup.ts",
    "email:to-workspace": "tsx scripts/email-to-workspace.ts",
    "email:to-workspace:ai": "tsx scripts/email-to-workspace-ai.ts",
    "ai:test": "tsx scripts/ai-test.ts",
    "seed:demo": "tsx prisma/seeds/demo-workspaces.ts",
    "ai:test:ceseda": "tsx scripts/ai-test-ceseda.ts",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "security:scan": "ggshield secret scan path .",
    "security:precommit": "ggshield secret scan pre-commit",
    "security:ci": "ggshield secret scan ci",
    "security:staged": "ggshield secret scan pre-receive",
    "security:install": "ggshield install -m local",
    "security:check": "ggshield secret scan path . --exit-zero",
    "pre-push": "npm run validate",
    "check-quality": "node scripts/check-quality.js",
    "security:audit": "npm audit && node scripts/security-monitor.js",
    "security:fix": "npm audit fix",
    "security:report": "node scripts/security-monitor.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@azure/identity": "^4.5.0",
    "@azure/keyvault-secrets": "^4.9.0",
    "@azure/storage-blob": "^12.24.0",
