{
  "name": "iapostemanage-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx @cloudflare/next-on-pages --watch",
    "pages:preview": "wrangler pages dev .vercel/output/static",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --watchAll=false",
    "analyze": "cross-env ANALYZE=true next build",
    "build:analyze": "npm run analyze",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next out dist",
    "figma": "node scripts/figma-cli.js",
    "figma:sync": "tsx scripts/figma-sync.ts",
    "figma:icons": "tsx scripts/figma-icons.ts",
    "figma:all": "npm run figma:sync && npm run figma:icons",
    "prepare": "echo Skipping husky install",
    "postinstall": "npx prisma generate",
    "db:reset": "node scripts/reset-db.js",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed:complete": "npx tsx prisma/seed-complete.ts",
    "db:studio": "npx prisma studio",
    "db:optimize": "tsx scripts/db-optimize.ts",
    "db:health": "tsx scripts/db-health.ts",
    "db:benchmark": "tsx scripts/db-benchmark.ts",
    "db:backup": "tsx scripts/db-backup.ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\"",
    "validate": "npm run type-check && npm run lint && npm run test:ci",
    "email:monitor": "tsx scripts/email-monitor.ts",
    "email:monitor:integrated": "tsx scripts/email-monitor-integrated.ts",
    "email:stats": "tsx scripts/email-stats.ts",
    "email:export": "tsx scripts/email-export.ts",
    "email:setup": "tsx scripts/email-setup.ts",
    "email:to-workspace": "tsx scripts/email-to-workspace.ts",
    "email:to-workspace:ai": "tsx scripts/email-to-workspace-ai.ts",
    "ai:test": "tsx scripts/ai-test.ts",
    "ai:test:ceseda": "tsx scripts/ai-test-ceseda.ts",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "security:scan": "ggshield secret scan path .",
    "security:precommit": "ggshield secret scan pre-commit",
    "security:ci": "ggshield secret scan ci",
    "security:staged": "ggshield secret scan pre-receive",
    "security:install": "ggshield install -m local",
    "security:check": "ggshield secret scan path . --exit-zero",
    "pre-push": "npm run validate",
    "check-quality": "node scripts/check-quality.js"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/pdf-parse": "^1.1.5",
    "@types/qrcode": "^1.5.6",
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "cloudflare": "^5.2.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "exceljs": "^4.4.0",
    "googleapis": "^169.0.0",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.562.0",
    "mailparser": "^3.9.1",
    "mammoth": "^1.11.0",
    "next": "^16.1.1",
    "next-auth": "^4.24.13",
    "openai": "^6.15.0",
    "otplib": "^12.0.1",
    "papaparse": "^5.5.3",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^4.0.0",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.69.0",
    "react-icons": "^5.5.0",
    "recharts": "^3.6.0",
    "regression": "^2.0.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.25.58"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@figma/code-connect": "^1.3.12",
    "@next/bundle-analyzer": "^15.1.6",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@svgr/core": "^8.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^25.0.3",
    "@types/nodemailer": "^7.0.4",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.6.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "figma-js": "^1.16.1-0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "^6.0.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.58.0"
  }
}
