{
  "version": 2,
  "name": "iaposte-manager-secure",
  "builds": [
    {
      "src": "app_secure.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app_secure.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_DEBUG": "False"
  },
  "regions": ["cdg1"]
}