# Configuration Docker - SecureVault v2.1

# Flask
FLASK_ENV=production
SECRET_KEY=changeme-in-production-use-long-random-string
FLASK_DEBUG=0

# OpenAI (optionnel)
OPENAI_API_KEY=

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Sécurité
RATE_LIMIT_ENABLED=1
CSRF_ENABLED=1

# Performance
CACHE_ENABLED=1
COMPRESSION_ENABLED=1

# Monitoring
METRICS_ENABLED=1
